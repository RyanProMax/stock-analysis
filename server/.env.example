# 股票分析系统 - 环境变量配置
# cp .env.example .env

# AI 分析（推荐 DeepSeek，性价比高）
DEEPSEEK_API_KEY=sk-your_deepseek_key_here
# OPENAI_API_KEY=sk-your_openai_key_here

# A 股数据源配置
# 优先级: Efinance(免费) -> Tushare(需Token) -> AkShare(免费) -> Pytdx(免费) -> Baostock(免费)

# Tushare Pro Token（可选，但推荐配置以提高优先级）
TUSHARE_TOKEN=your_tushare_token_here

# Pytdx 通达信服务器配置（可选）
# 使用逗号分隔多个服务器，格式: ip:port,ip:port
# PYTDX_SERVERS=119.147.212.81:7709,112.74.214.43:7727
# 或者配置单个服务器
# PYTDX_HOST=119.147.212.81
# PYTDX_PORT=7709

# 服务配置
ENV=development
PORT=8080
DEBUG=false
CACHE_DIR=.cache/

# 生产环境（可选）
# GCS_CACHE_BUCKET=your-gcs-bucket-name
